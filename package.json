{
    "name": "@cmmv/ai",
    "version": "0.0.6",
    "description": "CMMV module for LLM integration, tokenization, RAG dataset creation, and fast FAISS-based vector search for code indexing.",
    "author": "Andre Ferreira",
    "keywords": [
        "cmmv",
        "ai",
        "openai",
        "gemini",
        "llama",
        "cohere",
        "huggingface",
        "pinecone",
        "groq",
        "claude",
        "deepseek"
    ],
    "main": "./dist/cjs/main.js",
    "module": "./dist/esm/main.js",
    "types": "./dist/types/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/main.js",
            "require": "./dist/cjs/main.js"
        }
    },
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 20.0.0"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --ignore-path ./.prettierignore --write"
        ]
    },
    "scripts": {
        "build": "cmmv build",
        "release": "cmmv release",
        "lint": "cmmv lint",
        "prepare": "husky",
        "sample:tokenizer": "cmmv run ./samples/tokenizer.sample.ts",
        "sample:search": "cmmv run ./samples/search.sample.ts",
        "test": "vitest",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "devDependencies": {
        "@cmmv/cli": "^0.6.3",
        "@cmmv/testing": "^0.8.23",
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-angular": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@elastic/elasticsearch": "^8.17.0",
        "@langchain/aws": "^0.1.4",
        "@langchain/cohere": "^0.3.2",
        "@langchain/google-vertexai": "^0.2.0",
        "@langchain/mistralai": "^0.2.0",
        "@langchain/pinecone": "^0.1.3",
        "@neo4j/graphql": "^6.3.1",
        "@qdrant/js-client-rest": "^1.13.0",
        "@tsconfig/node22": "^22.0.0",
        "@types/node": "^22.13.5",
        "@types/pg": "^8.11.11",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "neo4j-driver": "^5.28.1",
        "pg": "^8.13.3",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "vitest": "^3.0.6"
    },
    "dependencies": {
        "@cmmv/core": "^0.8.23",
        "@langchain/anthropic": "^0.3.13",
        "@langchain/community": "^0.3.32",
        "@langchain/core": "^0.3.40",
        "@langchain/deepseek": "^0.0.1",
        "@langchain/google-genai": "^0.1.8",
        "@langchain/groq": "^0.1.3",
        "@langchain/ollama": "^0.2.0",
        "@langchain/openai": "^0.4.4",
        "@langchain/qdrant": "^0.1.1",
        "@langchain/textsplitters": "^0.1.0",
        "dotenv": "^16.4.7",
        "faiss-node": "^0.5.1",
        "fast-glob": "^3.3.3",
        "glob": "^11.0.1",
        "langchain": "^0.3.19",
        "uuid": "^11.1.0"
    }
}
